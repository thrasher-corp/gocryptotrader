// Start starts the {{.CapitalName}} go routine
func ({{.Variable}} *{{.CapitalName}}) Start(wg *sync.WaitGroup) {
	wg.Add(1)
	go func() {
		{{.Variable}}.Run()
		wg.Done()
	}()
}

